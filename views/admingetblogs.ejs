<div class="body">



<div class="container">
    <div class="text-center">
       <h1 class="pageTitleAdmin">Admin</h1>
    </div>
<br>
<div >
  <h3 class="pageTitleAdmin">All Blogs :</h3>
</div>
  <table class="table ">
        <thead class="thead-dark">
          <tr>
            
            <th  scope="col">First Name</th>
            <th  scope="col">Last Name</th>
            <th  scope="col">Title</th>
            <th  scope="col">Description</th>
            <th  scope="col">Delete</th>

        
            
          </tr>
        </thead>
       
       
            
            <% Blogs.forEach((blog) => { %>
                
                
        <table id="mytable" class="table table-striped ">     
        <tbody>
          <tr>
            
           
            <td  ><%= blog.fname %></td>
            <td  ><%= blog.lname %></td>
            <td  ><%= blog.title %></td>
            <td  ><%= blog.description %></td>
            <td  ><a href="/admin/blogs/delete/<%= blog._id%>"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
              <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z"/>
              <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z"/>
            </svg></a></td>

          </tr>
          <tr>       
        </tbody>
      </table>
    
<%  }) %>
</div>


</div>
</body>
