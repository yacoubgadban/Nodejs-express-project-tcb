    <div class="body">

      <div class="container">
    
        
            
            
          
    <div class="text-center">
      <img src="/images/nodejs.svg" alt="" width="200px">
    <h1 class="pageTitle">NodeJs Project</h1>
    </div>
    <br><br><br>
    
    
    
    
    <h4 class="pTitle"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-code" viewBox="0 0 16 16">
      <path d="M14 4.5V14a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h5.5L14 4.5zm-3 0A1.5 1.5 0 0 1 9.5 3V1H4a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V4.5h-2z"/>
      <path d="M8.646 6.646a.5.5 0 0 1 .708 0l2 2a.5.5 0 0 1 0 .708l-2 2a.5.5 0 0 1-.708-.708L10.293 9 8.646 7.354a.5.5 0 0 1 0-.708zm-1.292 0a.5.5 0 0 0-.708 0l-2 2a.5.5 0 0 0 0 .708l2 2a.5.5 0 0 0 .708-.708L5.707 9l1.647-1.646a.5.5 0 0 0 0-.708z"/>
    </svg> Programers</h4>
    <br>
    <br>
    <% if(User.length==0) {%>
    <div class="text-center">
    <h3>There is no users</h3>
    </div>
    <%}%>
    
    <div class="row row-cols-1 row-cols-md-4">
      <% User.forEach((user) => { %>
          <div class="col mb-4">
            <div class="card"> 
               
              <div class="card-body text-center" id="usercard"   >
                <div class="container text-center" >
                  <img src="/images/<%=user.imgurl%>" style="border:1px solid rgb(100, 197, 171)" altclass="card-img-top"  width="140px" height="200px" alt="...">
                </div>
                <h5 class="card-title" style="color:rgb(65, 155, 117)"><%=user.fname %> <%=user.lname %></h5>
                <hr>
                <p class="card-text" style="height:100px"><%= user.description %></p>
                <hr>                
                <p class="card-text" style="color:rgb(190, 102, 62)"><%=user.email %></p>  
                <hr>
                <p class="card-text" style="color:rgb(190, 102, 62)"><%=user.phone%></p>
                 
              </div>
            </div>      
          </div>        
      <%  }) %>    
    </div>
    <br><br>
    <h4 class="pTitle"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chat-quote" viewBox="0 0 16 16">
      <path d="M2.678 11.894a1 1 0 0 1 .287.801 10.97 10.97 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8.06 8.06 0 0 0 8 14c3.996 0 7-2.807 7-6 0-3.192-3.004-6-7-6S1 4.808 1 8c0 1.468.617 2.83 1.678 3.894zm-.493 3.905a21.682 21.682 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a9.68 9.68 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7-3.582 7-8 7a9.06 9.06 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105z"/>
      <path d="M7.066 6.76A1.665 1.665 0 0 0 4 7.668a1.667 1.667 0 0 0 2.561 1.406c-.131.389-.375.804-.777 1.22a.417.417 0 0 0 .6.58c1.486-1.54 1.293-3.214.682-4.112zm4 0A1.665 1.665 0 0 0 8 7.668a1.667 1.667 0 0 0 2.561 1.406c-.131.389-.375.804-.777 1.22a.417.417 0 0 0 .6.58c1.486-1.54 1.293-3.214.682-4.112z"/>
    </svg> blogs</h4>
    <br>
    
    <% if(Blogs.length==0) {%>
    <div id="blog" class="text-center">
    <h3>There is no blogs</h3><br><br><br>
    </div>
    <%}%>
    <% Blogs.forEach((blog) => { %>
    
    <div >
    <hr>
    <h5 id="blogheader" ><img src="/images/<%= blog.imgurl%>" style="border-radius: 50%;"  alt="" width="70px" height="100px "> <%=blog.fname %> <%= blog.lname%></h5>
        <div class="col mb-4">
          <div >   
           
            <div class="blogbody" >
              <h5  ><%=blog.title %></h5>
              <p ><%=blog.description %></p>
              <ul  class="list-group list-group-flush">   
                <li id="blogfooter" class="list-group-item" style="width:30rem;" ><%=blog.createdAt %></li>
              </ul>
           
            </div>
          </div>      
        </div>        
    <%  }) %>    
        <br>
        <br>
           
          
           
    
    </div>
    </div>
  </div>
</div>
</div>
